<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/webViewFullScreen"
        tools:targetApi="31">
        <activity
            android:name=".EmptyHome"
            android:exported="false" />
         <activity
            android:name=".app.users.views.activities.account.Registro_2"
            android:exported="false" />
        <activity
            android:name=".app.users.views.activities.account.Registro_1"
            android:exported="false" />
        <activity
            android:name=".app.party.views.activities.YourOrders"
            android:exported="false" />
        <activity
            android:name=".app.party.views.activities.ShowMoreParty"
            android:exported="false" />
        <activity
            android:name=".app.party.views.activities.PartyDetailActivity"
            android:exported="false" />
        <activity
            android:name=".app.main.views.activities.HomeActivity"
            android:exported="false" />
        <activity
            android:name=".app.users.views.activities.account.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".app.main.views.activities.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>